<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.cloud.traveler.dao.ProductDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.cloud.traveler.entity.ProductEntity" id="productMap">
        <result property="id" column="id"/>
        <result property="productname" column="productName"/>
        <result property="summarize" column="summarize"/>
        <result property="introduce" column="introduce"/>
        <result property="productlink" column="productLink"/>
        <result property="productimg" column="productImg"/>
        <result property="logo" column="logo"/>
        <result property="createtime" column="createTime"/>
        <result property="status" column="status"/>
    </resultMap>


</mapper>